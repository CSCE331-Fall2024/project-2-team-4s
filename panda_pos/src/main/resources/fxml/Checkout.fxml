  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <ListView fx:id="checkout_listing" layoutX="75.0" layoutY="63.0" prefHeight="243.0" prefWidth="513.0" />
            <Button id="confirm_checkout" fx:id="confirm_checkout" layoutX="486.0" layoutY="334.0" mnemonicParsing="false" onAction="#confirm_checkout" text="Confirm" />
            <Button id="go_back" fx:id="go_back" layoutX="410.0" layoutY="334.0" mnemonicParsing="false" onAction="#go_back" text="Go Back" />
      </children>
    </AnchorPane>
  </children>
</VBox>